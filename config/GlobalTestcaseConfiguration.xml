<?xml version="1.0" encoding="UTF-8"?>  
<beans xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
      
    xsi:schemaLocation="    
     http://www.springframework.org/schema/beans     
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd    
     ">  
  
    <bean class="com.sunpal.config.GlobalTestcaseConfiguration">  
        <!-- 配置信息 -->    
        <property name="loginUsername">  
            <value>username</value>  
        </property> 
        <property name="loginPassword">  
            <value>password</value>  
        </property> 
        <property name="waitTimeout">  
            <value>10000</value>  
        </property>
        <property name="upgradeWaitTime">  
            <value>60000</value>  
        </property>
        <property name="screenshotPath">  
            <value>/screenshots/</value>  
        </property>
    </bean>  
</beans> 